<div class="container">
  <div class="row">
    <div class="col-md-12 col-lg-12 col-xl-12">
      <h1><%= @user.first_name %> <%= @user.last_name %></h1>

      <h3>Tracked Auctions:</h3>
      <% @auctions.each do |a| %>
      <h4><%= link_to a.title, auction_path(a) %> - <%= a.aasm_state %></h4>
      <hr>
      <% end %>

    </div>
  </div>
</div>
